<table>
  <thead> This table dispalys the results of all your previous rounds:</thead>
  <tbody>
    <th><u> Round <u></th>
    <th><u> Deck Name <u></th>
    <th><u> Correct Answers <u></th>
    <th><u> Total Guesses <u></th>
    <th><u> Percent Score <u></th>
    <% @rounds.each do |round|%>
      <tr>
        <td><%=round.id%></u></td>
        <td><%=round.deck.name%></u></td>
        <td><%=round.guesses.where(correctness: 1).count%></td>
        <td><%=round.guesses.count%></td>
        <td><%=pctg(round)%>%</td>
      </tr>
    <%end%>
  </tbody>
</table>
