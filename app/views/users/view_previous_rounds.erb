<% @rounds.each do |round|%>
  <h3><u>Round <%=round.id%></u></h3>
  <h3>Correct Answers: <%=round.guesses.where(correctness: 1).count%></h3>
  <h3>Total Guesses: <%=round.guesses.count%></h3>
  <h3>Percent Score: <%=round.guesses.where(correctness: 1).count.to_f / round.guesses.count.to_f * 100%><br>
<%end%>
